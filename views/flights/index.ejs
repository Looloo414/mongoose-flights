<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <table id="list">
        <thead>
          <tr>
            <th>Title</th>
            <th>Flights</th>
            <th></th>
          </tr>
        </thead>
        <tbody>
          <% flights.forEach(function(f) { %>
            <tr>
              <td><%= f.airline %></td>
              <td><%= f.airport %></td>
              <td><%= f.flightNo %></td>
              <td><%= f.departs %></td>
            </tr>
        <% }); %>
          </tbody>
        </table>
</body>
</html>

